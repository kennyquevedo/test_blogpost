@model List<BlogPost.Domain.AppUser>
@{
    ViewData["Title"] = "User Manager";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h1>User Manager</h1>
<table class="table table-striped">
    <thead>
        <tr>
            <th>Id</th>
            <th>Username</th>
            <th>Edit</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Id</td>
                <td>@user.UserName</td>
                <td>
                    <a class="btn btn-primary" asp-controller="User" asp-action="Edit" asp-route-userId="@user.Id">Edit</a>
                </td>
            </tr>
        }
    </tbody>
</table>